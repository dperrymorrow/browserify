{
  "prompts": {
    "name": {
      "type": "string",
      "required": true,
      "label": "Project name"
    },
    "version": {
      "type": "string",
      "required": true,
      "label": "The version of the package",
      "default": "0.1.0"
    },
    "description": {
      "type": "string",
      "required": true,
      "label": "Project description",
      "default": "A Vue.js project"
    },
    "author": {
      "type": "string",
      "label": "Author"
    }
  },
  "skipInterpolation": "src/**/*.vue",
  "filters": {
    "test/e2e/**/*": "e2e"
  },
  "completeMessage": "To get started:\n\n  cd {{ destDirName }}\n  npm install\n  npm run local"
}
